{
  "formatVersion": 3,
  "id": "${mod_id}",
  "version": "${mod_version}",
  "name": "${mod_name}",
  "description": "${mod_desc}",
  "authors": [
    "darkmattrmaestro"
  ],
  "meta": {
    "icon": "${mod_id}:icons/icon.png"
  },
  "entrypoints": {
    "client-postInit": [
      "com.darkmattrmaestro.cosmic_tools.CosmicToolsPostinit"
    ]
  },
  "mixins": [
    "cosmic_tools.mixins.json",
    {
      "config": "cosmic_tools.client.mixins.json",
      "environment": "client"
    }
  ],
  "depends": {
    "cosmic-reach": ">=0.4.17-alpha",
    "puzzle-loader-cosmic": ">=1.3.4-alpha",
    "puzzle-loader-core": ">=1.0.12-alpha",
    "cosmic-api": ">=1.3.1-0.4.17"
  }
}
